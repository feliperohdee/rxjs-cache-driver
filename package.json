    "author": "Felipe Rohde",
    "license": "ISC",
    "dependencies": {
        "rxjs": "^6.6.3"
    },
    "devDependencies": {
        "chai": "^4.3.0",
        "eslint": "^7.20.0",
        "mocha": "^8.3.0",
        "nyc": "^15.1.0",
        "sinon": "^9.2.4",
        "sinon-chai": "^3.5.0"
    },
    "nyc": {
        "all": true,
        "reporter": "lcov",
        "exclude": [
            "*.spec.js"
        ]
    }
